{
	"name": "@lorhansohaky/itsdangerous",
	"version": "1.0.0",
	"description": "Safely pass trusted data to untrusted environments and back.",
	"keywords": [
		"hmac",
		"itsdangerous",
		"pallets",
		"signing",
		"security",
		"serialization",
		"digest"
	],
	"homepage": "https://github.com/LorhanSohaky/itsdangerous#readme",
	"bugs": {
		"url": "https://github.com/LorhanSohaky/itsdangerous/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LorhanSohaky/itsdangerous.git"
	},
	"license": "MIT",
	"author": "Hampus Kraft <hello@hampuskraft.com>",
	"sideEffects": false,
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"build": "tsc && tsup",
		"format": "prettier --write **/*.{md,yaml} && pnpm biome format --write . && pnpm biome check --write .",
		"lint": "biome lint --error-on-warnings --write .",
		"prepublishOnly": "npm run -s build",
		"postinstall": "lefthook install",
		"test": "vitest run"
	},
	"prettier": {
		"bracketSpacing": false,
		"printWidth": 120,
		"singleQuote": true,
		"useTabs": false
	},
	"dependencies": {
		"pako": "2.1.0",
		"uint8array-extras": "1.5.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.0",
		"@types/pako": "2.0.4",
		"lefthook": "2.0.4",
		"prettier": "3.6.2",
		"tsup": "8.5.0",
		"typescript": "5.9.3",
		"vite": "7.1.11",
		"vitest": "3.2.4"
	},
	"engines": {
		"node": ">=18"
	}
}
