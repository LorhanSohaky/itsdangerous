{
	"name": "itsdangerous.js",
	"version": "2.0.1",
	"description": "Safely pass trusted data to untrusted environments and back.",
	"keywords": ["hmac", "itsdangerous", "pallets", "python", "security", "serialization"],
	"homepage": "https://github.com/hampuskraft/itsdangerous.js#readme",
	"bugs": {
		"url": "https://github.com/hampuskraft/itsdangerous.js/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hampuskraft/itsdangerous.js.git"
	},
	"license": "MIT",
	"author": "Hampus Kraft <hello@hampuskraft.com>",
	"sideEffects": false,
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": ["dist", "LICENSE", "README.md"],
	"scripts": {
		"build": "tsc && tsup",
		"format": "prettier --write **/*.{md,yaml} && pnpm biome format --write . && pnpm biome check --write .",
		"lint": "biome lint --write --unsafe .",
		"prepublishOnly": "npm run -s build",
		"test": "vitest run"
	},
	"prettier": {
		"bracketSpacing": false,
		"printWidth": 120,
		"singleQuote": true,
		"useTabs": true
	},
	"dependencies": {
		"pako": "2.1.0",
		"uint8array-extras": "1.4.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@types/pako": "2.0.3",
		"prettier": "3.3.3",
		"tsup": "8.2.4",
		"typescript": "5.5.4",
		"vite": "5.4.2",
		"vitest": "2.0.5"
	},
	"engines": {
		"node": ">=18"
	}
}
